{
    "name": "@navikt/smart-on-fhir",
    "version": "1.1.1",
    "packageManager": "yarn@4.7.0",
    "engines": {
        "node": "22"
    },
    "type": "module",
    "scripts": {
        "build": "tsup",
        "test": "vitest",
        "tsc": "tsc",
        "lint": "biome check",
        "format": "biome format --write",
        "postinstall": "husky",
        "next:major": "yarn version -i major",
        "next:minor": "yarn version -i minor"
    },
    "publishConfig": {
        "access": "public",
        "registry": "https://npm.pkg.github.com/"
    },
    "files": [
        "dist",
        "README.md",
        "LICENSE"
    ],
    "exports": {
        "./client": {
            "types": "./dist/client/index.d.mts",
            "import": "./dist/client/index.mjs"
        },
        "./zod": {
            "types": "./dist/zod/index.d.mts",
            "import": "./dist/zod/index.mjs"
        }
    },
    "dependencies": {
        "@navikt/pino-logger": "^4.1.5",
        "@opentelemetry/api": "^1.9.0",
        "jose": "^6.0.11",
        "openid-client": "^6.6.2",
        "pino": "^9.7.0",
        "zod": "^3.25.67"
    },
    "devDependencies": {
        "@biomejs/biome": "2.0.6",
        "@types/node": "^24.0.10",
        "@vitest/coverage-v8": "3.2.4",
        "husky": "^9.1.7",
        "lint-staged": "^16.1.2",
        "nock": "^14.0.5",
        "pinst": "^3.0.0",
        "prettier": "^3.6.2",
        "tsup": "^8.5.0",
        "typescript": "^5.8.3",
        "vitest": "^3.2.4"
    }
}

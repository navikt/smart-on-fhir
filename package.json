{
    "name": "@navikt/smart-on-fhir",
    "version": "1.5.5",
    "packageManager": "yarn@4.10.2",
    "engines": {
        "node": "24"
    },
    "type": "module",
    "scripts": {
        "build": "tsup",
        "test": "vitest",
        "tsc": "tsc",
        "lint": "biome check",
        "format": "biome format --write",
        "format:markdown": "bunx prettier --write docs/**/*.md",
        "postinstall": "husky",
        "next:major": "yarn version -i major",
        "next:minor": "yarn version -i minor",
        "docs:dev": "vitepress dev docs",
        "docs:build": "vitepress build docs",
        "docs:preview": "vitepress preview docs"
    },
    "publishConfig": {
        "access": "public",
        "registry": "https://npm.pkg.github.com/"
    },
    "files": [
        "dist",
        "README.md",
        "LICENSE"
    ],
    "exports": {
        "./client": {
            "types": "./dist/client/index.d.ts",
            "import": "./dist/client/index.js"
        },
        "./zod": {
            "types": "./dist/zod/index.d.ts",
            "import": "./dist/zod/index.js"
        }
    },
    "dependencies": {
        "@opentelemetry/api": "^1.9.0",
        "@opentelemetry/core": "^2.1.0",
        "jose": "^6.1.0",
        "openid-client": "^6.8.1",
        "quick-lru": "^7.2.0"
    },
    "peerDependencies": {
        "@navikt/pino-logger": "4",
        "pino": "9 || 10",
        "zod": "4"
    },
    "devDependencies": {
        "@biomejs/biome": "2.2.5",
        "@karl-run/prettier-config": "^1.1.0",
        "@navikt/pino-logger": "^4.3.3",
        "@types/node": "24.6.2",
        "@vitest/coverage-v8": "3.2.4",
        "husky": "^9.1.7",
        "lint-staged": "^16.2.3",
        "nock": "^14.0.10",
        "pino": "^10.0.0",
        "pinst": "^3.0.0",
        "prettier": "^3.6.2",
        "tsup": "^8.5.0",
        "typescript": "^5.9.3",
        "vitepress": "^1.6.4",
        "vitepress-plugin-npm-commands": "^0.8.1",
        "vitepress-plugin-tabs": "^0.7.3",
        "vitest": "^3.2.4",
        "zod": "^4.1.11"
    },
    "prettier": "@karl-run/prettier-config"
}

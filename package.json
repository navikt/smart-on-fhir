{
    "name": "@navikt/smart-on-fhir",
    "version": "1.4.17",
    "packageManager": "yarn@4.9.2",
    "engines": {
        "node": "22"
    },
    "type": "module",
    "scripts": {
        "build": "tsup",
        "test": "vitest",
        "tsc": "tsc",
        "lint": "biome check",
        "format": "biome format --write",
        "format:markdown": "bunx prettier --write docs/**/*.md",
        "postinstall": "husky",
        "next:major": "yarn version -i major",
        "next:minor": "yarn version -i minor",
        "docs:dev": "vitepress dev docs",
        "docs:build": "vitepress build docs",
        "docs:preview": "vitepress preview docs"
    },
    "publishConfig": {
        "access": "public",
        "registry": "https://npm.pkg.github.com/"
    },
    "files": [
        "dist",
        "README.md",
        "LICENSE"
    ],
    "exports": {
        "./client": {
            "types": "./dist/client/index.d.ts",
            "import": "./dist/client/index.js"
        },
        "./zod": {
            "types": "./dist/zod/index.d.ts",
            "import": "./dist/zod/index.js"
        }
    },
    "dependencies": {
        "@navikt/pino-logger": "^4.1.5",
        "@opentelemetry/api": "^1.9.0",
        "@opentelemetry/core": "^2.0.1",
        "jose": "^6.1.0",
        "openid-client": "^6.7.0",
        "pino": "^9.9.0",
        "quick-lru": "^7.1.0",
        "zod": "^4.1.5"
    },
    "devDependencies": {
        "@biomejs/biome": "2.2.2",
        "@karl-run/prettier-config": "^1.1.0",
        "@types/node": "22.18.0",
        "@vitest/coverage-v8": "3.2.4",
        "husky": "^9.1.7",
        "lint-staged": "^16.1.5",
        "nock": "^14.0.10",
        "pinst": "^3.0.0",
        "prettier": "^3.6.2",
        "tsup": "^8.5.0",
        "typescript": "^5.9.2",
        "vitepress": "^1.6.4",
        "vitepress-plugin-npm-commands": "^0.8.1",
        "vitepress-plugin-tabs": "^0.7.1",
        "vitest": "^3.2.4"
    },
    "prettier": "@karl-run/prettier-config"
}
